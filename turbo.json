{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "./dist/**"
      ]
    },
    "lint": {
      "dependsOn": []
    },
    "check-types-transit": {
      "dependsOn": [
        "^check-types-transit"
      ]
    },
    "check-types": {
      "dependsOn": [
        "check-types-transit"
      ]
    }
  }
}